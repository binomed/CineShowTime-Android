	  android:layout_height="wrap_content">
  </LinearLayout>
  <LinearLayout
	  xmlns:android="http://schemas.android.com/apk/res/android"
	  android:id="@+id/root_container"
	  android:layout_width="fill_parent"
	  android:layout_height="fill_parent">
  </LinearLayout>
</LinearLayout>
