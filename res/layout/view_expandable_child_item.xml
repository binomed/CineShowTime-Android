<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/object_sub_item" 
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	>
	<TextView  
	    android:id="@+id/object_sub_name"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    style="?attr/cstObjectSubMainInfo"
	    />
	<TextView  
	    android:id="@+id/object_sub_subcontent_name"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:layout_below="@id/object_sub_name"
	    />
</RelativeLayout>
